fn test2() -> Array<int> {
    let n = 10;
    let arr = new Array(n, 0);
    let i = 0;
    while i < n {
        let j = i + 1;
        while j > 0 {
            arr[i] += j;
            j -= 1;
        }
        i += 1;
    }
    return arr;
}

