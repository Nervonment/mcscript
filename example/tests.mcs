fn while_1() -> int {
    let i = 5;
    let j = 7;
    while i < 100 {
        i += 30;
        while j < 100 {
            j += 6;
        }
        j -= 100;
    }
    return j;
}

fn while_2() -> int {
    let a = 5;
    let b = 6;
    let c = 7;
    let d = 10;
    while a < 20 {
        a += 3;
        while b < 10 {
            b += 1;
            while c == 7 {
                c -= 1;
                while d < 20 {
                    d += 3;
                }
                d -= 1;
            }
            c += 1;
        }
        b -= 2;
    }
    return a + (b + d) + c;
}

fn while_3() -> int {
  let a = 5;
  let b = 6;
  let c = 7;
  let d = 10;
  let g = 1;
  let h = 2;
  let e = 4;
  let f = 6;
  while a < 20 {
    a = a + 3;
    while b < 10 {
      b = b + 1;
      while c == 7 {
        c = c - 1;
        while d < 20 {
          d = d + 3;
          while e > 1 {
            e = e - 1;
            while f > 2 {
              f = f - 2;
              while g < 3 {
                g = g + 10;
                while h < 10 {
                  h = h + 8;
                }
                h = h - 1;
              }
              g = g - 8;
            }
            f = f + 1;
          }
          e = e + 1;
        }
        d = d - 1;
      }
      c = c + 1;
    }
    b = b - 2;
  }
  return (a + (b + d) + c) - (e + d - g + h);
}

fn break_1() -> int {
    let i = 0;
    let sum = 0;
    while i < 100 {
        if i == 50 {
            break;
        }
        sum += i;
        i += 1;
    }
    return sum;
}

fn continue_1() -> int {
    let i = 0;
    let sum = 0;
    while i < 100 {
        if i == 50 {
            i += 1;
            continue;
        }
        sum += i;
        i += 1;
    }
    return sum;
}

