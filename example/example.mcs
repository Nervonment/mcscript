fn fib(n) {
    if n < 2 {
        return 1;
    }
    let x = n;
    x = x - 1;
    return fib(x) + fib(n - 2);
}

fn sum(n) {
    let s = 0;
    while n > 0 {
        s = s + n;
        n = n - 1;
    }
    return s;
}

fn sum_sum(n) {
    let s = 0;
    while n > 0 {
        let s1 = 0;
        let i = n;
        while i > 0 {
            s1 = s1 + i;
            i = i - 1;
        }
        s = s + s1;
        n = n - 1;
    }
    return s;
}

fn main() {
    return sum_sum(50);
}